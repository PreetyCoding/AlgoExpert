import java.util.*;

class Program {
  // Feel free to add new properties and methods to the class.
  static class MinMaxStack {
    public int peek() {
      // Write your code here.
			if(!values.isEmpty())
				return values.peek();
      return -1;
    }

    public int pop() {
      // Write your code here.
			if(!values.isEmpty()){
				int number = values.peek();
				if(!min.isEmpty() && number == min.peek())
						min.pop();
				if(!max.isEmpty() && number == max.peek())
						max.pop();
				return values.pop();
			}
      return -1;
    }

    public void push(Integer number) {
      // Write your code here.
			values.push(number);
			if(min.isEmpty() || number<=min.peek())
					min.push(number);
			if(max.isEmpty() || number>=max.peek())
					max.push(number);
    }

    public int getMin() {
      // Write your code here.
			if(!min.isEmpty())
					return min.peek();
      return -1;
    }

    public int getMax() {
      // Write your code here.
			if(!max.isEmpty())
					return max.peek();
      return -1;
    }
		
		Stack<Integer> values = new Stack<>();
		Stack<Integer> min = new Stack<>();
		Stack<Integer> max = new Stack<>();
  }
}
